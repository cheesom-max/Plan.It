{
    "version": 2,
    "builds": [
        {
            "src": "api/*.js",
            "use": "@vercel/node"
        },
        {
            "src": "*.html",
            "use": "@vercel/static"
        },
        {
            "src": "*.css",
            "use": "@vercel/static"
        },
        {
            "src": "*.js",
            "use": "@vercel/static"
        },
        {
            "src": "images/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/search-cities",
            "dest": "/api/search-cities.js"
        },
        {
            "src": "/api/generate-itinerary",
            "dest": "/api/generate-itinerary.js"
        },
        {
            "src": "/(.*\\.css)",
            "dest": "/$1"
        },
        {
            "src": "/(.*\\.js)",
            "dest": "/$1"
        },
        {
            "src": "/(.*\\.html)",
            "dest": "/$1"
        },
        {
            "src": "/images/(.*)",
            "dest": "/images/$1"
        },
        {
            "src": "/",
            "dest": "/index.html"
        }
    ]
}